    return 0;
}
<!--Exercise-->
#include <stdio.h>

int main() {
    int a = 1;
    if (a == 1)
        printf("a: %d", a);
    return 0;
}
<!--Exercise-->
#include <stdio.h>

int main() {
    int a = 1;
    if (a == 1) {
        printf("a: %d", a);
    }
    return 0;
}
<!--Exercise-->
#include <stdio.h>

int main() {
    float a = 1;
    float b = 2;
    float c = a/b;
    printf("c: %d", c);
    printf("c: %f", c);
    return 0;
}
