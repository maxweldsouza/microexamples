if a > 10
    print "a is greater than 10"
<!--Exercise-->
a = 12
if a > 10:
    print "a is greater than 10"
<!--Exercise-->
a = 12
if a > 10:
print "a is greater than 10"
<!--Exercise-->
day = "sunday"
if day = "sunday":
    print "take a nap"
<!--Exercise-->
day = "sunday"
if day == "sunday":
    print "take a nap"
