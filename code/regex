<!--Chapter=basics-->
<!--Exercise-->
import re
input = "penn and teller, bullshit"
regex = r"and"

print re.findall(regex, input);
<!--Exercise-->
import re
input = "i am the stig"
regex = r"i"

print re.findall(regex, input);
<!--Exercise-->
import re
input = "this is 5 abc"
regex = r"5"

print re.findall(regex, input);
<!--Exercise-->
import re
input = "this is 5 67abc"
regex = r"[0-9]"

print re.findall(regex, input);
<!--Chapter=Any Character-->
<!--Exercise-->
import re
input = "ab 6b 5b ac cc dc"
regex = r".b"

print re.findall(regex, input);
<!--Exercise-->
import re
input = "ab cb db ac cc dc"
regex = r"b."

print re.findall(regex, input);
<!--Exercise-->
import re
input = "bab b5b beb bub bba bbc"
regex = r"b.b"

print re.findall(regex, input);
